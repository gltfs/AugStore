#
# Configuration file for DICOM viewer common commands.
#
# Do not change files in the config/config directory. These are default template files.
# If you want to edit a configuration file, copy the default template file from the config/config
# directory to the config/local_config directory and modify the file copy.
#
# Please note that it is required to test any configuration changes. This is done by:
#
# 1. Export an examination and the DICOM viewer to a disk location.  
# 2. Run the viewer from the disk location and verify that the changes have taken effect.
#
# If the viewer doesn't start properly it is likely that the changes to the configuration 
# file were made incorrectly.
# To remedy such problems, remove the modified file from the config/local_config directory.
#
# If the default template files are corrupted, remove the entire config directory 
# including sub folders "config" and "local_config". 
# The folder structure will be re-created during the next export from IDS7.
# 

Commands {
    about_cmd {
	Library <nt_common_cmd.dll>
	Factory_name <create_about_cmd>
	Label <About IDS...>
    }
    context_help_cmd {
	Library <nt_common_cmd.dll>
	Factory_name <create_context_help_cmd>
	Label <&On Context...>
	ToolbarLabel <Context>
	Tips <Display help based on selected context>
	AcceleratorKey <o>
	AcceleratorModifier <SI_CTRL_L>    
	AcceleratorLabel <Ctrl+o>    
	Pixmap <toolbar\help_on_context.bmp>
	PixmapHigh <toolbar\high\help_on_context.bmp>
    }

    sys_adm_guide_cmd {
	Library <nt_common_cmd.dll>
	Factory_name <create_sysadm_guide_help_cmd>
	Label <System Administrator's guide...>
        ToolbarLabel <IDS5>
	Tips <Open System Administrator's guide>
	Pixmap <toolbar\help_index.bmp>
	PixmapHigh <toolbar\high\help_index.bmp> 
    }

    nop_cmd {
	Library <nt_common_cmd.dll>
	Factory_name <create_nop_cmd>
    }
    neverop_cmd {
	Library <nt_common_cmd.dll>
	Factory_name <create_neverop_cmd>
    }
    quit {
	Library <nt_common_cmd.dll>
	Factory_name <create_quit_cmd>
	Label <E&xit>
        ToolbarLabel <Exit>
        Tips <Exit and logout>
	Pixmap <toolbar\exit.bmp>
	PixmapHigh <toolbar\high\exit.bmp>
    }
    ref_manual_cmd {
	Library <nt_common_cmd.dll>
	Factory_name <create_ref_guide_help_cmd>
	Label <Reference Manual...>
        ToolbarLabel <Reference>
	Tips <Display reference manual>
	Pixmap <toolbar\reference_guide.bmp>
	PixmapHigh <toolbar\high\reference_guide.bmp>
    }
    undo_cmd {
	Library <nt_common_cmd.dll>
	Factory_name <create_undo_cmd>
	Label <&Undo>
        ToolbarLabel <Undo>
	Tips <Undo the latest operation>
	AcceleratorKey <u>
	AcceleratorModifier <SI_CTRL_L>    
	AcceleratorLabel <Ctrl+u>    
	Pixmap <toolbar\undo.bmp>
	PixmapHigh <toolbar\high\undo.bmp>
    }
    users_guide_cmd {
	Library <nt_common_cmd.dll>
	Factory_name <create_users_guide_help_cmd>
	Label <User's Guide...>
        ToolbarLabel <Help>
	AcceleratorKey <h>
	AcceleratorModifier <SI_CTRL_L>    
	AcceleratorLabel <Ctrl+h>    
	Tips <Open IDS5 user's guide>
        Pixmap <toolbar\help.bmp>
        PixmapHigh <toolbar\high\help.bmp>
    }
    minimize_windows_cmd {
	Library <nt_common_cmd.dll>
	Factory_name <create_minimize_window_cmd>
	Label <&Minimize windows>
        ToolbarLabel <Minimize>
	Tips <Minimizes all window for this application>
	Pixmap <toolbar\minimize_windows.bmp>
	PixmapHigh <toolbar\high\minimize_windows.bmp>
    }
    format_date_cmd {
	date_type <american1>
	Library <nt_common_cmd.dll>
	Factory_name <create_format_date_cmd>
    }
    format_archive_date_cmd {
	number_of_chars <8>
	Library <nt_common_cmd.dll>
	Factory_name <create_format_archive_date_cmd>
    }
    format_archive_state_cmd {
	Library <nt_common_cmd.dll>
	Factory_name <create_format_archive_state_cmd>
    }
    sectra_homepage_cmd {
	Library <nt_common_cmd.dll>
	Factory_name <create_sectra_homepage_cmd>
	Label <Sectra Homepage>
	ToolbarLabel <Sectra>
	Tips <Sectra Homepage>
	Pixmap <toolbar\homepage.bmp>
	PixmapHigh <toolbar\high\homepage.bmp>
   }
}
